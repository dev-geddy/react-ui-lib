{
  "presets": [
    [
      "env",
      {
        "debug": true,
        "targets": {
          "node": "current",
          "browsers": [
            "> 1%",
            "last 3 versions",
            "ios > 8",
            "not ie < 10"
          ]
        },
        "useBuiltIns": true
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "lodash"
  ]
}